return this._super(...arguments);},_checkURL:function(){const defaultURL='https://www.facebook.com/Odoo';const match=this.fbData.href.trim().match(/^(https?:\/\/)?((www\.)?(fb|facebook)|(m\.)?facebook)\.com\/(((profile\.php\?id=|people\/([^/?#]+\/)?|(p\/)?[^/?#]+-)(?<id>[0-9]{12,16}))|(?<nameid>[\w.]+))($|[/?# ])/);if(match){const pageId=match.groups.nameid||match.groups.id;return fetch(`https://graph.facebook.com/${pageId}/picture`).then((res)=>{if(res.ok){this.fbData.id=pageId;}else{this.fbData.id="";this.fbData.href=defaultURL;this.displayNotification({title:_t("We couldn't find the Facebook page"),type:"warning",});}});}
