odoo.define('website.s_table_of_content_options',function(require){'use strict';const options=require('web_editor.snippets.options');options.registry.TableOfContent=options.Class.extend({start:function(){this.targetedElements='h1, h2';this.oldHeadingsEls=[];this.oldHeadingsDesktopVisible=[];const $headings=this.$target.find(this.targetedElements);if($headings.length>0){this._generateNav();}
